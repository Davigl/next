{
  "extends": ["@rocketseat/eslint-config/next", "next/core-web-vitals"],
  "plugins": ["import-helpers"],
  "rules": {
    "no-unused-vars": "off",
    "import-helpers/order-imports": [
      "warn",
      {
        "newlinesBetween": "always",
        "groups": [
          "/^react/",
          "module",
          "/^~/",
          ["parent", "sibling", "index"]
        ],
        "alphabetize": {
          "order": "asc",
          "ignoreCase": true
        }
      }
    ],
    "@typescript-eslint/no-explicit-any": "off"
  }
}
